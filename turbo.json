{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", "prisma/generated/**"],
			"env": [
				"DATABASE_URL",
				"BETTER_AUTH_SECRET",
				"BETTER_AUTH_URL",
				"CORS_ORIGIN",
				"STRIPE_SECRET_KEY",
				"STRIPE_WEBHOOK_SECRET",
				"STRIPE_PRICE_ID_STARTER_MONTHLY",
				"STRIPE_PRICE_ID_STARTER_ANNUAL",
				"STRIPE_PRICE_ID_PRO_MONTHLY",
				"STRIPE_PRICE_ID_PRO_ANNUAL",
				"STRIPE_PRICE_ID_SCALE_MONTHLY",
				"STRIPE_PRICE_ID_SCALE_ANNUAL",
				"GOOGLE_CLIENT_ID",
				"GOOGLE_CLIENT_SECRET",
				"RESEND_API_KEY",
				"RESEND_EMAIL_DOMAIN",
				"RESEND_FROM_EMAIL",
				"APP_NAME",
				"EMAIL_LOGO_URL",
				"TIGRIS_ENDPOINT_URL",
				"TIGRIS_ACCESS_KEY_ID",
				"TIGRIS_SECRET_ACCESS_KEY",
				"TIGRIS_BUCKET_NAME",
				"TIGRIS_PUBLIC_URL",
				"BLOB_READ_WRITE_TOKEN",
				"OPENROUTER_API_KEY",
				"OPENROUTER_DEFAULT_MODEL",
				"OPENROUTER_TEMPLATE_MODEL",
				"UPSTASH_REDIS_REST_URL",
				"UPSTASH_REDIS_REST_TOKEN",
				"FIRECRAWL_API_KEY",
				"FAL_API_KEY",
				"FAL_BASE_URL",
				"FAL_IMAGE_MODEL",
				"FAL_IMAGE_EDIT_MODEL",
				"FAL_IMAGE_RATE_PER_MINUTE",
				"FAL_IMAGE_RATE_PER_DAY",
				"FAL_IMAGE_ENABLED",
				"NODE_ENV"
			]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"db:push": {
			"cache": false,
			"persistent": true
		},
		"db:studio": {
			"cache": false,
			"persistent": true
		},
		"db:migrate": {
			"cache": false,
			"persistent": true
		},
		"db:generate": {
			"cache": false,
			"persistent": true
		},
		"db:migrate:deploy": {
			"cache": false,
			"env": ["DATABASE_URL"]
		}
	}
}
