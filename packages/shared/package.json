{
  "name": "@mocah/shared",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "default": "./src/logger.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@upstash/redis": "catalog:"
  },
  "devDependencies": {
    "@mocah/config": "workspace:*",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}

