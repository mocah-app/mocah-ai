{
  "name": "@mocah/db",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "postinstall": "prisma generate --schema=./prisma/schema/schema.prisma",
    "prebuild": "prisma generate --schema=./prisma/schema/schema.prisma",
    "build": "tsdown",
    "db:push": "prisma db push --schema=./prisma/schema/schema.prisma",
    "db:studio": "prisma studio --schema=./prisma/schema/schema.prisma",
    "db:generate": "prisma generate --schema=./prisma/schema/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=./prisma/schema/schema.prisma"
  },
  "devDependencies": {
    "@mocah/config": "workspace:*",
    "prisma": "^6.19.0",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/client": "catalog:",
    "dotenv": "catalog:",
    "zod": "catalog:"
  }
}
